{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "space-before-function-paren": "off",
    "no-console": "off",
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "import/prefer-default-export": "off",
    "no-return-assign": "off",
    "consistent-return": "off",
    "no-param-reassign": "off",
    "eslint-disable-next-line": "off",
    "quotes": [
      "error",
      "double"
    ],
    "import/extensions": "off"
  }
}